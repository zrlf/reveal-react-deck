{
  "name": "reveal-react-deck",
  "version": "0.0.2",
  "description": "",
  "scripts": {
    "build": "pnpm build:layout && pnpm build:mdx-plugins",
    "build:layout": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:mdx-plugins": "tsup",
    "clean": "rimraf dist",
    "dev": "concurrently \"tsc -w --project tsconfig.build.json\" \"tsc-alias -w -p tsconfig.build.json\"",
    "lint": "eslint .",
    "types:check": "tsc --noEmit",
    "preversion": "pnpm run build"
  },
  "exports": {
    ".": "./dist/index.js",
    "./config": "./dist/config.js",
    "./plugins": "./dist/plugins.mjs",
    "./style.css": "./dist/style.css",
    "./css/*": "./css/*",
    "./components/*": "./dist/components/*",
    "./hooks/*": "./dist/hooks/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.9.0",
  "files": [
    "dist/*",
    "css/*"
  ],
  "devDependencies": {
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.15.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/reveal.js": "^5.0.5",
    "acorn": "^8.14.1",
    "bibtex": "^0.9.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "estree-util-value-to-estree": "^3.3.3",
    "js-yaml": "^4.1.0",
    "katex": "^0.16.22",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reveal.js": "^5.2.1",
    "tsc-alias": "^1.8.15",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "@mdx-js/react": "^3.1.0",
    "react": "^19",
    "react-dom": "^19",
    "reveal.js": "^5.2.1",
    "tailwindcss": "^4"
  },
  "dependencies": {
    "framer-motion": "^12.9.1",
    "tailwind-merge": "^3.2.0",
    "zustand": "^5.0.3"
  }
}
